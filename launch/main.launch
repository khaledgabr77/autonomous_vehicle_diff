<?xml version="1.0"?>
<launch>
  <param name="/use_sim_time" value="true" />

  <!-- start world -->


  <node name="gazebo" pkg="gazebo_ros" type="gazebo" 
   args="$(find urdf_tests)/world/car_1_junction.world" respawn="false" output="screen" />

  <!-- start gui -->
 <node name="gazebo_gui" pkg="gazebo" type="gui" respawn="false" output="screen"/> 

<!--node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" /-->
  
  <node pkg="rviz" type="rviz" name="rviz" required="true" args="-d $(find urdf_tests)/rviz/amcl_config.rviz"/>
  <!-- include our robots -->
 <include file="$(find urdf_tests)/launch/multi_agent/robots.launch"/>

</launch>
